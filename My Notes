
//////////////////////////////////////////////////////////
//////////  ALL CLEAR TEXT FUNCTION  ////////////////////
////////////////////////////////////////////////////////

import org.openqa.selenium.Keys as Keys


WebUI.sendKeys(findTestObject('Object Element'), Keys.chord(Keys.CONTROL, 'a'))
WebUI.sendKeys(findTestObject('Object Element'), Keys.chord(Keys.BACK_SPACE))

WebUI.clearText(findTestObject('Object Element'))

